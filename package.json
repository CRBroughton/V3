{
  "private": true,
  "name": "v3",
  "version": "0.1.0",
  "scripts": {
    "build": "nuxt build",
    "dev": "pnpm run prisma:generate && nuxt dev",
    "check": "NPM_CHECK_INSTALLER=pnpm npm-check -u",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "analyze": "npx nuxi analyze",
    "postinstall": "nuxt prepare",
    "prisma:generate": "npx prisma generate",
    "prisma:db:push": "npx prisma db push",
    "prisma:db:pull": "npx prisma db pull",
    "prisma:migrate": "npx prisma migrate dev",
    "prisma:seed": "pnpm prisma:db:push --force-reset && npx prisma db seed",
    "prisma:reset": "npx prisma migrate reset",
    "prisma:studio": "npx prisma studio",
    "changeset": "npx changeset",
    "changeset:status": "npx changeset status --verbose",
    "changeset:version": "npx changeset version",
    "test:prepare": "pnpm prisma:db:push --force-reset && npx prisma db seed",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:watch": "vitest --watch",
    "test:server": "vitest --dir tests/server",
    "test:components": "vitest --dir tests/components"
  },
  "prisma": {
    "seed": "ts-node-dev -P tsconfig.server.json prisma/seed.ts"
  },
  "devDependencies": {
    "@antfu/eslint-config": "0.36.0",
    "@changesets/cli": "^2.26.0",
    "@nuxt/devtools": "^0.2.5",
    "@nuxt/test-utils": "^3.2.3",
    "@sidebase/nuxt-auth": "^0.4.3",
    "@types/node": "18.15.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitest/ui": "^0.29.2",
    "@vue/test-utils": "^2.3.1",
    "autoprefixer": "^10.4.14",
    "eslint": "8.36.0",
    "jsdom": "^21.1.0",
    "npm-check": "^6.0.1",
    "nuxt": "3.2.3",
    "postcss": "8.4.21",
    "prisma": "4.11.0",
    "tailwindcss": "3.2.7",
    "ts-node-dev": "^2.0.0",
    "typescript": "4.9.5",
    "vitest": "^0.29.2",
    "vue-tsc": "1.2.0"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.5",
    "@prisma/client": "4.11.0",
    "@trpc/client": "10.15.0",
    "@trpc/server": "10.15.0",
    "next-auth": "4.20.1",
    "superjson": "1.12.2",
    "trpc-nuxt": "0.7.0",
    "ts-pattern": "4.2.1",
    "zod": "3.21.4"
  }
}
